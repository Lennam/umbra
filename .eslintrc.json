{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "semi": 1,
        "space-in-parens": 1,
        "space-before-function-paren": 1,
        "space-infix-ops": 1,
        "space-unary-ops": 1,
        "space-before-blocks": 1
    }
}